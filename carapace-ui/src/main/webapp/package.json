{
  "name": "carapace-ui",
  "version": "1.3.0-SNAPSHOT",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && rm -rf ui && mv dist ui",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.1.0 ",
    "moment": "2.24.0",
    "vue": "2.6.10",
    "vue-cli": "2.9.6",
    "vue-router": "3.1.3",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/vue-fontawesome": "0.1.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.7.0",
    "@vue/cli-plugin-eslint": "3.7.0",
    "@vue/cli-service": "3.7.0",
    "node-sass": "4.12.0",
    "sass-loader": "7.1.0",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
